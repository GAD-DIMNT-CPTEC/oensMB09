label "section 1";
# START grib2::section
# SECTION 1, IDENTIFICATION SECTION
#  Length of section in octets
# (21 or nn)
position offsetSection1;
length[4] section1Length ;

#  Number of section
unsigned[1] numberOfSection = 1 :read_only;

#  Identification of originating/generating centre
# (see Common Code Table C-1)
codetable[2] identificationOfOriginatingGeneratingCentre  'grib1/0.table';
alias ls.centre=identificationOfOriginatingGeneratingCentre;

#  Identification of originating/generating sub-centre
# (allocated by originating/generating Centre)
unsigned[2] identificationOfOriginatingGeneratingSubCentre ;
alias subCentre=identificationOfOriginatingGeneratingSubCentre;

#  GRIB Master Tables Version Number
# (currently 2)
# NOTE 1 NOT FOUND
codetable[1] gribMasterTablesVersionNumber 'grib2/1.0.table' = 4;

#  Version number of GRIB Local Tables used to augment Master Tables
# NOTE 2 NOT FOUND
codetable[1] versionNumberOfGribLocalTables 'grib2/1.1.table';

#  Significance of Reference Time
codetable[1] significanceOfReferenceTime 'grib2/1.2.table' = 1; # significanceOfReferenceTime

#  Year
# (4 digits)
unsigned[2] year ;

#  Month
unsigned[1] month ;

#  Day
unsigned[1] day ;

#  Hour
unsigned[1] hour ;

#  Minute
unsigned[1] minute ;

#  Second
unsigned[1] second ;

meta    dataDate    g2date(year,month,day);
alias mars.date = dataDate;
alias ls.date = dataDate;

meta      dataTime    time(hour,minute,second);
alias mars.time = dataTime;


#  Production status of processed data in this GRIB message
codetable[1] productionStatusOfProcessedData 'grib2/1.3.table';

#  Type of processed data in this GRIB message
codetable[1] typeOfProcessedData 'grib2/1.4.table';

alias ls.dataType=typeOfProcessedData;

# Octets 22-nn : Reserved: need not be present
# ????          reserved_need_not_be_present

# END   grib2::section
