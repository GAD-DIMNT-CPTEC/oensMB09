EXEC=RoughnessLengthClima

.SUFFIXES: .o .f90 .F90 .c 
#
# *****************************************************************
#

OBJS = 	Watches.o\
	RoughnessLengthClima.o

$(EXEC): $(OBJS)
	$(F90) -o $(EXEC) $(F90FLAG) $(LOADFLAG) $(OBJS)

Watches.o:               Watches.f90
	$(F90) -c $(F90FLAG) Watches.f90

RoughnessLengthClima.o: 	RoughnessLengthClima.f90 Watches.o
	$(F90) -c $(F90FLAG) RoughnessLengthClima.f90

SRC	= $(OBJ_MOD:.o=.f90) $(OBJS:.o=.f90) $(OBJS_CC:.o=.c)

clean:
	-rm -f $(OBJS)
	-rm -f $(OBJS_CC)
	-rm -f $(EXEC)
	-rm -f *.mod

.F90.o :
	$(F90) $(F90FLAG) -c  $<

.f90.o:
	$(F90) $(F90FLAG) -c $< 
.c.o:
	$(CC) $(CFLAGS) $(DEFS) -c $< 
