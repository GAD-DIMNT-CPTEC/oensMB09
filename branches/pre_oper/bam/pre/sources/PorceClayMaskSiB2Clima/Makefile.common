EXEC=PorceClayMaskSiB2Clima

OBJS = 	PorceClayMaskSiB2Clima.o

$(EXEC):	$(OBJS)
	$(F90) -o $(EXEC) $(F90FLAG) $(LOADFLAG) $(OBJS)

PorceClayMaskSiB2Clima.o: 	PorceClayMaskSiB2Clima.f90
	$(F90) -c $(F90FLAG) PorceClayMaskSiB2Clima.f90

clean:
	-rm -f $(OBJS)
	-rm -f $(OBJS_CC)
	-rm -f $(EXEC)
	-rm -f *.mod

.F90.o :
	$(F90) $(F90FLAG) -c  $<

.f90.o:
	$(F90) $(F90FLAG) -c $< 
.c.o:
	$(CC) $(CFLAGS) $(DEFS) -c $< 
