if(mmm=APR);_mm=04;endif;
if(mmm=MAY);_mm=05;endif;
if(mmm=JUN);_mm=06;endif;
if(mmm=JUL);_mm=07;endif;
if(mmm=AUG);_mm=08;endif;
if(mmm=SEP);_mm=09;endif;
if(mmm=OCT);_mm=10;endif;
if(mmm=NOV);_mm=11;endif;
if(mmm=DEC);_mm=12;endif;
*say "The input value was : "mmm
*say "The output values is: "_mm
return 
