  plevs(11:20) = 875.00,  850.00,  825.00,  800.00,  775.00,  750.00,  725.00,  700.00,  675.00,  650.00,
  plevs(21:30) = 600.00,  500.00,  300.00,  250.00,  200.00,  150.00,  100.00,   70.00,   50.00,   30.00,
  plevs(31:33) =  20.00,   10.00,    3.0,     0.00
 /

